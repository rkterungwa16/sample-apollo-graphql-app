{
  "moduleFileExtensions": ["js", "json", "ts", "tsx"],
  "resetMocks": true,
  "rootDir": "src",
  "testRegex": "/.*\\.test\\.tsx?$",
  "transform": {
    "^.+\\.(t|j)s$": "babel-jest",
    "^.+\\.(jsx|tsx)$": "babel-jest"
  },
  "testPathIgnorePatterns": ["<rootDir>/node_modules/"],
  "transformIgnorePatterns": ["^.+\\.module\\.(css|sass|scss)$"],
  "coverageDirectory": "../coverage",
  "testEnvironment": "jsdom"
}
